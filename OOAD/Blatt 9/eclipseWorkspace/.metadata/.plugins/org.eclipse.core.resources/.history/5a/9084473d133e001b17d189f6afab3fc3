package Ausruestung;

public abstract class Ausruestung {
	protected String name;
	protected int preis;

	public Ausruestung(String name, int preis) {
		this.name = name;
		this.preis = preis;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setPreis(int preis) {
		this.preis = preis;
	}

	public int getPreis() {
		return preis;
	}
}
