package Kaempfer;

public abstract class Kaempfer {
	protected int gesundheit;
	protected int geschick;
	protected int sold;
	
	public Kaempfer(int gesundheit, int geschick, int sold) {
		this.gesundheit = gesundheit;
		this.geschick = Math.max(0, Math.min(geschick, 10));
		this.sold = sold;
		
		
	}
}
